{
  "name": "danilin-em/memcached",
  "description": "Memcached client for PHP. No dependencies, no extensions, no third-party libraries. No regrets.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Evgenii Danilin",
      "email": "evgenii.danilin.m@gmail.com"
    }
  ],
  "homepage": "https://github.com/danilin-em/test-memcached-lib",
  "require": {
    "php": "^5.6",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Memcached\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Memcached\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpunit": "XDEBUG_IS_ACTIVE=1 XDEBUG_TRIGGER=XDEBUG_IS_ACTIVE XDEBUG_MODE=coverage,debug phpunit -c phpunit.xml",
    "ci": [
      "@phpunit"
    ]
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
